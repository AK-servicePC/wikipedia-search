{
"manifest_version": 2,
	"name": "Wikipedia Search",
	"version": "6.2",
	"description": "Search Wikipedia in every language from the address bar.",
	"options_page": "settings.html",
	"omnibox": {
		"keyword": "wiki"
	},
	"web_accessible_resources": [
		"widget.html",
		"settings.html"
	],
	"permissions": [
		"contextMenus",
		"http://*.wikipedia.org/*",
		"https://*.wikipedia.org/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"icons":{
		"16":"icon16.png",
		"32":"icon32.png",
		"48":"icon48.png",
		"128":"icon128.png"
	}
}